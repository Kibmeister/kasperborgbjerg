,CAAA,sBAAA,EAAA,KAAA;AACA,WAAA,gBAAA;AACA,KAhEA;AAiEA,IAAA,gBAjEA,8BAiEA;AACA,WAAA,QAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,gBAAA;AACA,KAnEA;AAoEA,IAAA,QApEA,oBAoEA,CApEA,EAoEA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,gCAAA,CAAA,CAAA,OAAA,GAAA,OAAA;AACA;AAtEA;AAhCA,CAAA","sourcesContent":["<template>\n  <div ref=\"draggableContainer\" id=\"resizable\" class=\"draggableContainer\" v-bind:style='{\"zIndex\" : (zIndex), \"visibility\" : (isActive? \"visible\" : \"hidden\" ),\n     \"width\" : (windowExpanded? \"70%\" : \"\"), \"height\" : (windowExpanded? \"70%\" : \"\")}'>\n\n    <div class=\"draggable-header\" @mousedown=\"dragMouseDown\">\n      <div class=\"draggable-header-text\">{{header}}</div>\n      <div class=\"title-bar-controls\">\n        <Button aria-label=\"Maximize\" v-on:click=\"windowExpanded = !windowExpanded\">\n        </Button>\n        <Button aria-label=\"Close\" v-on:click=\"hideContainer\"></Button>\n      </div>\n    </div>\n    <div class=\"content\">{{content}}</div>\n  </div>\n\n</template>\n\n<script>\nexport default {\n  name: \"ItemWindow\",\n  props: {\n    header: String,\n    content: String,\n    zIndex: Number,\n    id: String\n  },\n  data: function() {\n    return {\n      positions: {\n        clientX: undefined,\n        clientY: undefined,\n        movementX: 0,\n        movementY: 0,\n        offset: {\n          x: 0,\n          y: 0\n        },\n        dragoffset: {\n          x: 0,\n          y: 0\n        }\n      },\n      isActive: true,\n      windowExpanded: false,\n      expandSize: {\n        width: \"70%\",\n        height: \"70%\"\n      }\n    };\n  },\n  methods: {\n    dragMouseDown: function(event) {\n      event.preventDefault();\n      this.$store.commit(\"setGlobalZindex\", this.id);\n      //this.$refs.draggableContainer.style.zIndex = this.$store.getters.globalZindex;\n      //console.log(\"Global zIndex : \" + this.$store.getters.globalZindex);\n\n      this.positions.clientX = event.clientX;\n      this.positions.clientY = event.clientY;\n      this.positions.dragoffset.x =\n        event.clientX - this.$refs.draggableContainer.offsetLeft;\n      this.positions.dragoffset.y =\n        event.clientY - this.$refs.draggableContainer.offsetTop;\n\n      document.onmousemove = this.elementDrag;\n      document.onmouseup = this.closeDragElement;\n    },\n    elementDrag: function(event) {\n      event.preventDefault();\n      this.positions.movementX = this.positions.clientX - event.clientX; // setter movementX = gamleX - nyX\n      this.positions.movementY = this.positions.clientY - event.clientY; // setter movementY = gamleY - nyY\n      this.positions.clientX = event.clientX;\n      this.positions.clientY = event.clientY;\n\n      // left/right constraints\n      if (event.clientX - this.positions.dragoffset.x < 0) {\n        this.positions.offset.x = 0;\n      } else if (\n        event.clientX - this.positions.dragoffset.x + 300 >\n        this.$store.getters.bodyWidth\n      ) {\n        this.positions.offset.x = this.$store.getters.bodyWidth - 300;\n      } else {\n        this.positions.offset.x = event.clientX - this.positions.dragoffset.x;\n      }\n      // top/bottom constraints\n      if (event.clientY - this.positions.dragoffset.y < 0) {\n        this.positions.offset.y = 0;\n      } else if (\n        event.clientY - this.positions.dragoffset.y + 300 >\n        this.$store.getters.bodyHeight\n      ) {\n        this.positions.offset.y = this.$store.getters.bodyHeigth - 300;\n      } else {\n        this.positions.offset.y = event.clientY - this.positions.dragoffset.y;\n      }\n\n      this.$refs.draggableContainer.style.top = this.positions.offset.y + \"px\";\n      this.$refs.draggableContainer.style.left = this.positions.offset.x + \"px\";\n\n      let payload = {\n        id: this.id,\n        top: this.positions.offset.y,\n        left: this.positions.offset.x\n      };\n      this.$store.commit(\"setWindowTopLeft\", payload);\n    },\n    closeDragElement() {\n      document.onmouseup = null;\n      document.onmousemove = null;\n    },\n    hideContainer() {\n      this.$store.commit(\"setAboutMeVisibility\", false);\n      this.toggleVisibility();\n    },\n    toggleVisibility() {\n      this.isActive = this.$store.getters.aboutMeVisiblity;\n    },\n    onResize(e) {\n      console.log(\"the div is resizing with : \" + e.content + \"width\");\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.draggableContainer {\n  position: absolute;\n  width: 300px;\n  height: 350px;\n  border: black solid 2px;\n  background-color: white;\n  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.29);\n  resize: both;\n  overflow: hidden;\n  transition-timing-function: ease-in-out;\n  transition: width, height;\n  transition-duration: 100ms;\n\n  .draggable-header {\n    max-width: auto;\n    height: 22px;\n    border-bottom-style: solid;\n    border-bottom-color: black;\n    border-bottom-width: 2px;\n    background-color: #fbfbef;\n    font: bold 14px \"Helvetica Neue\", Arial, Helvetica, Geneva, sans-serif;\n\n    .draggable-header-text {\n      padding: 2px;\n      display: absolute;\n    }\n\n    .title-bar-controls {\n      margin: 4px 4px 0px 0px;\n      position: absolute;\n      float: right;\n      top: 0px;\n      right: 0px;\n      button {\n        cursor: url(\"https://i.imgur.com/m5J1hiZ.png\"), auto;\n      }\n    }\n  }\n  .content {\n    padding: 5px;\n  }\n}\n</style>"],"sourceRoot":"src/components"}]}