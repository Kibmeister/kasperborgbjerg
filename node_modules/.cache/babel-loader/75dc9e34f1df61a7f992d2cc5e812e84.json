IAAA;AACA,MAAA,QAAA,CAAA,WAAA,GAAA,IAAA;AACA,KA/DA;AAgEA,IAAA,aAhEA,2BAgEA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,6BAAA,EAAA,KAAA;AACA,WAAA,gBAAA;AACA,KAnEA;AAoEA,IAAA,YApEA,0BAoEA;AAAA;;AACA,WAAA,cAAA,GAAA,CAAA,KAAA,cAAA;AAEA,WAAA,WAAA,GACA,KAAA,UAAA,CAAA,KAAA,IAAA,KAAA,KAAA,CAAA,kBAAA,CAAA,WADA;AAEA,WAAA,YAAA,GACA,KAAA,UAAA,CAAA,MAAA,IAAA,KAAA,KAAA,CAAA,kBAAA,CAAA,YADA;;AAGA,UAAA,KAAA,cAAA,EAAA;AACA,aAAA,KAAA,CAAA,kBAAA,CAAA,KAAA,CAAA,KAAA,GAAA,KAAA,GAAA;AACA,aAAA,KAAA,CAAA,kBAAA,CAAA,KAAA,CAAA,MAAA,GAAA,KAAA,GAAA;AACA,OAHA,MAGA,IACA,CAAA,KAAA,cAAA,KACA,KAAA,YAAA,IAAA,KAAA,WADA,CADA,EAGA;AACA,aAAA,KAAA,CAAA,kBAAA,CAAA,KAAA,CAAA,KAAA,GAAA,KAAA,GAAA;AACA,aAAA,KAAA,CAAA,kBAAA,CAAA,KAAA,CAAA,MAAA,GAAA,KAAA,GAAA;AACA,aAAA,cAAA,GAAA,IAAA;AACA,OAPA,MAOA;AACA,aAAA,KAAA,CAAA,kBAAA,CAAA,KAAA,CAAA,KAAA,GAAA,EAAA;AACA,aAAA,KAAA,CAAA,kBAAA,CAAA,KAAA,CAAA,MAAA,GAAA,EAAA;AACA;;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA,CAAA,kBAAA,CAAA,WAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,MAAA,GAAA,KAAA,CAAA,KAAA,CAAA,kBAAA,CAAA,YAAA;AACA,OAHA,EAGA,GAHA,CAAA;AAIA,KA9FA;AA+FA,IAAA,gBA/FA,4BA+FA,EA/FA,EA+FA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,kBAAA,EAAA,EAAA;AACA;AAjGA;AAjCA,CAAA","sourcesContent":["<template>\n  <div ref=\"draggableContainer\" \n    class=\"draggableContainer\" \n    v-bind:style='{\"zIndex\" : (zIndex), \"visibility\" : (isActive? \"visible\" : \"hidden\" )}'\n    @click=\"zIndexToggle\">\n    <div class=\"draggable-header\" @mousedown=\"dragMouseDown\">\n      <div class=\"draggable-header-text\">{{header}}</div>\n      <div class=\"title-bar-controls\">\n        <Button aria-label=\"Maximize\" v-on:click=\"windowToggle\"></Button>\n        <Button aria-label=\"Close\" v-on:click=\"hideContainer\"></Button>\n      </div>\n    </div>\n    <div class=\"content\">\n      <ProjectItem ref=\"topico\" :url=\"this.$store.state.projectsFolderItems[0].url\" :id=\"this.$store.state.projectsFolderItems[0].id\" :subtitle=\"this.$store.state.projectsFolderItems[0].subtitle\" v-on:click.native=\"toggleVisibility('id_topico')\"> </ProjectItem>\n      <ProjectItem ref=\"smartBelt\" :url=\"this.$store.state.projectsFolderItems[1].url\" :id=\"this.$store.state.projectsFolderItems[1].id\" :subtitle=\"this.$store.state.projectsFolderItems[1].subtitle\" v-on:click.native=\"toggleVisibility('id_smartBelt')\"> </ProjectItem>\n      <ProjectItem ref=\"cleaver\" :url=\"this.$store.state.projectsFolderItems[2].url\" :id=\"this.$store.state.projectsFolderItems[2].id\" :subtitle=\"this.$store.state.projectsFolderItems[2].subtitle\" v-on:click.native=\"toggleVisibility('id_cleaver')\"> </ProjectItem>\n    </div>\n  </div>\n\n</template>\n\n<script>\nexport default {\n  name: \"ItemWindowProjects\",\n  props: {\n    header: String,\n    zIndex: Number,\n    id: String\n  },\n  data: function() {\n    return {\n      positions: {\n        clientX: undefined,\n        clientY: undefined,\n        movementX: 0,\n        movementY: 0,\n        offset: {\n          x: 0,\n          y: 0\n        },\n        dragoffset: {\n          x: 0,\n          y: 0\n        }\n      },\n      isActive: true,\n      windowExpanded: false,\n      windowSize: {\n        width: 0,\n        height: 0\n      },\n      widthChange: true,\n      heightChange: true\n    };\n  },\n  methods: {\n    zIndexToggle (){\n      this.$store.commit(\"setGlobalZindex\", this.id);\n    },\n    dragMouseDown: function(event) {\n      event.preventDefault();\n      this.zIndexToggle();  \n      // get the mous1e cursor position at startup:\n\n      this.positions.clientX = event.clientX;\n      this.positions.clientY = event.clientY;\n      this.positions.dragoffset.x =\n        event.clientX - this.$refs.draggableContainer.offsetLeft;\n      this.positions.dragoffset.y =\n        event.clientY - this.$refs.draggableContainer.offsetTop;\n\n      document.onmousemove = this.elementDrag;\n      document.onmouseup = this.closeDragElement;\n    },\n    elementDrag: function(event) {\n      event.preventDefault();\n\n      this.positions.movementX = this.positions.clientX - event.clientX; // setter movementX = gamleX - nyX\n      this.positions.movementY = this.positions.clientY - event.clientY; // setter movementY = gamleY - nyY\n      this.positions.clientX = event.clientX;\n      this.positions.clientY = event.clientY;\n\n      // left/right constraints\n      if (event.clientX - this.positions.dragoffset.x < 0) {\n        this.positions.offset.x = 0;\n      } else if (\n        event.clientX - this.positions.dragoffset.x + 300 >\n        this.$store.getters.bodyWidth\n      ) {\n        this.positions.offset.x = this.$store.getters.bodyWidth - 300;\n      } else {\n        this.positions.offset.x = event.clientX - this.positions.dragoffset.x;\n      }\n      // top/bottom constraints\n      if (event.clientY - this.positions.dragoffset.y < 0) {\n        this.positions.offset.y = 0;\n      } else if (\n        event.clientY - this.positions.dragoffset.y + 300 >\n        this.$store.getters.bodyHeight\n      ) {\n        this.positions.offset.y = this.$store.getters.bodyHeigth - 300;\n      } else {\n        this.positions.offset.y = event.clientY - this.positions.dragoffset.y;\n      }\n\n      this.$refs.draggableContainer.style.top = this.positions.offset.y + \"px\";\n      this.$refs.draggableContainer.style.left = this.positions.offset.x + \"px\";\n\n      let payload = {\n        id: this.id,\n        top: this.positions.offset.y,\n        left: this.positions.offset.x\n      };\n      this.$store.commit(\"setWindowTopLeft\", payload);\n    },\n    closeDragElement() {\n      document.onmouseup = null;\n      document.onmousemove = null;\n    },\n    hideContainer() {\n      this.$store.commit(\"setProjectsWindowVisibility\", false);\n      this.toggleVisibility();\n    },\n     windowToggle() {\n      this.windowExpanded = !this.windowExpanded;\n\n      this.widthChange =\n        this.windowSize.width != this.$refs.draggableContainer.clientWidth;\n      this.heightChange =\n        this.windowSize.height != this.$refs.draggableContainer.clientHeight;\n\n      if (this.windowExpanded) {\n        this.$refs.draggableContainer.style.width = 70 + \"%\";\n        this.$refs.draggableContainer.style.height = 70 + \"%\";\n      } else if (\n        !this.windowExpanded &&\n        (this.heightChange || this.widthChange)\n      ) {\n        this.$refs.draggableContainer.style.width = 70 + \"%\";\n        this.$refs.draggableContainer.style.height = 70 + \"%\";\n        this.windowExpanded = true;\n      }  else {\n        this.$refs.draggableContainer.style.width = \"\";\n        this.$refs.draggableContainer.style.height = \"\";\n      }\n      setTimeout(() => {\n        this.windowSize.width = this.$refs.draggableContainer.clientWidth;\n        this.windowSize.height = this.$refs.draggableContainer.clientHeight;\n      }, 110);\n    },\n     toggleVisibility(id) {\n      this.$store.commit('setWindowVisible', id)\n  }   \n\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.draggableContainer {\n  position: absolute;\n  width: 400px;\n  height: 300px;\n  border: black solid 2px;\n  background-color: white;\n  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.29);\n  resize: both;\n  overflow: hidden;\n  transition-timing-function: ease-in-out;\n  transition: width, height;\n  transition-duration: 100ms;\n\n  .draggable-header {\n    max-width: auto;\n    max-height: 22px;\n    border-bottom-style: solid;\n    border-bottom-color: black;\n    border-bottom-width: 2px;\n    background-color: #fbfbef;\n    font: bold 14px \"Helvetica Neue\", Arial, Helvetica, Geneva, sans-serif;\n\n    .draggable-header-text {\n      padding: 2px;\n      display: absolute;\n    }\n\n    .title-bar-controls {\n      margin: 4px 4px 0px 0px;\n      position: absolute;\n      float: right;\n      top: 0px;\n      right: 0px;\n      button {\n        cursor: url(\"https://i.imgur.com/m5J1hiZ.png\"), auto;\n      }\n    }\n  }\n  .content {\n    padding: 5px;\n    display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(100px, 1fr));\n  }\n}\n</style>"],"sourceRoot":"src/components"}]}