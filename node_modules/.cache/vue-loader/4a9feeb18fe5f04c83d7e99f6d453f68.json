 '../mixins/windowScript'\nexport default {\n  name: 'ItemWindow',\n  props: {\n    header: String,\n    intro: String,\n    zIndex: Number,\n    id: String,\n    width: Number,\n    height: Number\n  },\n  data: function() {\n    return {\n      positions: {\n        clientX: undefined,\n        clientY: undefined,\n        movementX: 0,\n        movementY: 0,\n        offset: {\n          x: 0,\n          y: 0\n        },\n        dragoffset: {\n          x: 0,\n          y: 0\n        }\n      },\n      isActive: true\n    };\n  },\n  mixins: [windowScript]\n};\n</script>\n\n<style lang=\"scss\" scoped>\n\n@import '@/scss/variables';\n\n.draggableContainer {\n  position: absolute;\n  min-width: 200px;\n  border: black solid 2px;\n  background-color: white;\n  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.29);\n  resize: both;\n  overflow: hidden;\n  overflow-y: scroll;\n  -ms-overflow-style: none;\n  scrollbar-width: none;\n  transition-timing-function: ease-in-out;\n  transition: width, height;\n  transition-duration: 100ms;\n\n  .draggableHeader {\n    position: fixed;\n    width: inherit;\n    min-width: inherit;\n    height: 20px;\n    border-bottom-style: solid;\n    border-bottom-color: black;\n    border-bottom-width: 2px;\n    background-color: #fbfbef;\n    font: bold 14px 'Helvetica Neue', Arial, Helvetica, Geneva, sans-serif;\n    z-index: 1000;\n    .draggable-header-text {\n      padding: 2px;\n      display: absolute;\n    }\n\n    .title-bar-controls {\n      display: flex;\n      margin: 0px 4px 0px 0px;\n      padding-top: 2px;\n      position: absolute;\n      float: right;\n      top: 2px;\n      right: 0px;\n      button {\n        cursor: url('https://i.imgur.com/m5J1hiZ.png'), auto;\n        padding: 0;\n        display: block;\n        min-width: 16px;\n        min-height: 14px;\n        margin-left: 1px;\n        background: $surface;\n        box-shadow: $border-raised-outer, $border-raised-inner;\n      }\n      button:not(:disabled):active {\n        box-shadow: $border-sunken-outer), var($border-sunken-inner);\n      }\n\n      button[aria-label='Minimize'] {\n        background-image: url('../assets/icons/minimize.svg');\n        background-repeat: no-repeat;\n        background-position: bottom 3px left 3px;\n        border: none;\n      }\n\n      button[aria-label='Maximize'] {\n        background-image: url('../assets/icons/maximize.svg');\n        background-repeat: no-repeat;\n        background-position: top 2px left 3px;\n        border: none;\n      }\n      button[aria-label='Close'] {\n        background-image: url('../assets/icons/close.svg');\n        background-repeat: no-repeat;\n        background-position: top 3px left 4px;\n        border: none;\n      }\n    }\n  }\n  .content {\n    position: relative;\n    text-align: left;\n    padding: 10px;\n    padding-top: 25px;\n    \n  }\n}\n</style>\n"]}]}